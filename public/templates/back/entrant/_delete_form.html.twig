<form method="post" action="{{ path('back_entrant_delete', {'id': entrant.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ entrant.id) }}">
    <button class="btn">Delete</button>
</form>
